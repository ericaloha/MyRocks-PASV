--source include/have_innodb.inc

SET GLOBAL innodb_lra_pages_before_sleep = 128;
SELECT @@GLOBAL.innodb_lra_pages_before_sleep;
SET SESSION innodb_lra_pages_before_sleep=1000000;
SELECT @@SESSION.innodb_lra_pages_before_sleep;
SET SESSION innodb_lra_pages_before_sleep=0;
SELECT @@SESSION.innodb_lra_pages_before_sleep;
SET SESSION innodb_lra_pages_before_sleep=16384;
SELECT @@SESSION.innodb_lra_pages_before_sleep;
SET GLOBAL innodb_lra_pages_before_sleep=-1;
SELECT @@GLOBAL.innodb_lra_pages_before_sleep;
SET GLOBAL innodb_lra_pages_before_sleep = default;
SELECT @@GLOBAL.innodb_lra_pages_before_sleep;
